{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/add-entry\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/change-password\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/signin\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/signout\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/signup\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/entries\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/users\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/change-password\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/dashboard\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/delete-user\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/profile\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/signin\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/signup\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/update-user\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}